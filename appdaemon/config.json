{
    "name": "HassDash2",
    "version": "0.0.1",
    "slug": "hassdash2",
    "description": "Master version of HADaemon (https://github.com/home-assistant/appdaemon/tree/hadashboard_beta)",
    "url": "https://github.com/wroadd/hassio-addons",
    "startup": "after",
    "image": "wroadd/{arch}-hassdash",
    "boot": "auto",
    "map": ["config:rw"],
    "options": {
        "domain": "cp.local.io"
    },
    "schema": {
        "domain": "str"
    }, 
    "ports": {
        "80/tcp": 3030
    }
}
